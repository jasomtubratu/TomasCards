{
  "expo": {
    "name": "TomasCards",
    "slug": "tomascards",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/icon.png",
      "usesNonExemptEncryption": false,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs camera access to scan barcodes and QR codes.",
        "NSLocationWhenInUseUsageDescription": "We need your location for the internal React Native libraries, even if we don't use it directly."
      },
      "bundleIdentifier": "com.tentomasne.tomas-cards"
    },
    "android": {
      "versionCode": 3,
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA"
      ],
      "package": "com.tentomasne.tomascards"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "recordAudioAndroid": false
        }
      ],
      "react-native-vision-camera",
      [
        "react-native-cloud-storage",
        {
          "iCloudContainerEnvironment": "Production",
          "iCloudContainerIdentifier": "iCloud.tomascards.icloud"
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "kawut"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2f98ca16-1cd1-44d2-8e4b-fa63d3e71a4f"
      }
    },
    "owner": "tomasovucet"
  }
}
